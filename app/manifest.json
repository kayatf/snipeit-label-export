{
  "name": "SnipeIT Label Extractor",
  "short_name": "SLT",
  "description": "Extracts labels generated by the SnipeIT app to a zip file.",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["storage", "cookies", "*://*/*"],
  "browser_specific_settings": {
    "gecko": {
      "id": "daniel.riegler@essteyr.com"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://inventory.essteyr.com/*/bulkedit"],
      "js": ["scripts/contentScript.js"]
    }
  ]
}
