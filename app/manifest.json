{
  "name": "SnipeIT Label Extractor",
  "short_name": "SLT",
  "description": "Extracts labels generated by the SnipeIT app to a zip file.",
  "version": "0.0.2",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://inventory.essteyr.com/*/bulkedit"
      ],
      "js": [
        "scripts/contentScript.js"
      ]
    }
  ]
}
